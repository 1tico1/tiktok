# Tokens e Chaves de API
API_TOKEN=supersecreto
YT_API_KEY=SEU_TOKEN

# Configurações S3
S3_ENDPOINT=
S3_BUCKET=tiktok-automation
S3_REGION=
S3_ACCESS_KEY=
S3_SECRET_KEY=

# Configurações TikTok
TIKTOK_APP_ID=
TIKTOK_APP_SECRET=
TIKTOK_ACCESS_TOKEN=

# Configurações do Servidor
PORT=3000
NODE_ENV=development

# Configurações de CORS
CORS_ORIGIN=http://localhost:5173  # Domínio do painel frontend
CORS_METHODS=GET,POST,PUT,DELETE,OPTIONS
CORS_CREDENTIALS=true

# Configurações de Erro
ERROR_STACK_TRACE=true  # Habilita stack trace nos erros em desenvolvimento

# Configurações de Rate Limiting
RATE_LIMIT_WINDOW=15m
RATE_LIMIT_MAX=100
